<!-- ************************************************************************************ -->
	<application>
		<resource-bundle>
			<base-name>br.gov.caixa.siavs.messages</base-name>
			<var>msgs</var>
		</resource-bundle>
		<locale-config>
			<default-locale>pt_BR</default-locale>
			<supported-locale>pt_BR</supported-locale>
			<supported-locale>en_US</supported-locale>
		</locale-config>
	</application>
	<factory>
		<exception-handler-factory>br.com.spread.framework.excecao.jsf.FactoryTratadorExcecaoJSF</exception-handler-factory>
	</factory>
<!-- ************************************************************************************ -->
</faces-config>
